-if current_user.nil? and current_member.nil?
 :plain
  window.location.replace("/sign-in");
-elsif session[:tracker_id].nil?
 :plain
  window.location.replace("/trackers");
-else
 :plain
  $('#tracker_requirement').attr('value','');
  $('#add_req_tracker').prepend('#{escape_javascript(render(:partial=>'requirements/create_requirement_tracker'))}');