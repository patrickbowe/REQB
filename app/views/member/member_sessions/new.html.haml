%h2="Member Login page"

-form_for @member_session, :url => member_member_sessions_path do |f|
    = f.error_messages
    = f.label :email
    %br
    = f.text_field :email
    %br
    %br    
    = f.label :password
    %br
    = f.password_field :password
    %br
    %br
    = f.check_box :remember_me
    = f.label :remember_me 
    %br
    = f.submit "Login"
